{
    "mcpServers": {
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "."
            ]
        },
        "seo_server": {
            "command": "uv",
            "args": [
                "run",
                "seo_server.py"
            ]
        },
        "fetch": {
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ]
        },
        "google": {
            "command": "uvx",
            "args": [
                "--env-file=.env",
                "workspace-mcp",
                "--tools",
                "sheets",
                "docs",
                "--tool-tier",
                "complete"
            ]
        }
    }
}